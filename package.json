{
  "name": "easy-component",
  "displayName": "easy-component",
  "description": "be easy to create your component structrue",
  "version": "1.2.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "publisher":"zem",
  "activationEvents": [
    "onCommand:extension.createComponent"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zhanzizhen/easy-component.git"
  },
  "author": "zhanzizhen",
  "main": "./out/extension.js",
  "icon": "images/cat.png",
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "extension.createComponent",
        "title": "Create Component Structure"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vsce:publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
